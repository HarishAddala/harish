pipeline {
    agent any
    stages {
        
        stage('compile stage') {
          steps {
              withMaven(maven : 'maven') {
              sh 'mvn -f pets clean compile'
              }
          }
        }

        stage('Test') {
          steps {
              withMaven(maven : 'maven') {
              sh 'mvn -f pets test'
              }
          }
        }

        stage('deploy') {
          steps {
              withMaven(maven : 'maven') {
              sh 'mvn -f pets deploy'
              }
          }
        }
    }
}